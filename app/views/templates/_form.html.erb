<%= bootstrap_form_for(template) do |f| %>
  <% if template.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(template.errors.count, "error") %> prohibited this template from being saved:</h2>

      <ul>
      <% template.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.text_area :file1 %>
  </div>

  <div class="field">
    <%= f.text_field :status %>
  </div>

  <div class="field">
    <%= f.number_field :user_id %>
  </div>

  <div class="field">
    <%= f.text_field :encode_code %>
  </div>

  <div class="field">
    <%= f.number_field :current_assignment_id %>
  </div>

  <div class="field">
    <%= f.number_field :assignment_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
